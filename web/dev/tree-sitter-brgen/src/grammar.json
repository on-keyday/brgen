{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "brgen",
  "rules": {
    "program": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "skip_lines"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_statement"
              },
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "skip_white"
                  },
                  {
                    "type": "BLANK"
                  }
                ]
              }
            ]
          }
        }
      ]
    },
    "skip_lines": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "space"
          },
          {
            "type": "SYMBOL",
            "name": "comment"
          },
          {
            "type": "SYMBOL",
            "name": "line"
          }
        ]
      }
    },
    "skip_white": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "space"
          },
          {
            "type": "SYMBOL",
            "name": "comment"
          },
          {
            "type": "SYMBOL",
            "name": "line"
          }
        ]
      }
    },
    "skip_space": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "space"
          },
          {
            "type": "SYMBOL",
            "name": "comment"
          }
        ]
      }
    },
    "_statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "expr"
        }
      ]
    },
    "space": {
      "type": "PATTERN",
      "value": "[ \\t]+"
    },
    "line": {
      "type": "PATTERN",
      "value": "[\\r\\n]+"
    },
    "comment": {
      "type": "PATTERN",
      "value": "#.*[\\r\\n]*"
    },
    "expr": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_unary"
        }
      ]
    },
    "unary_op": {
      "type": "PATTERN",
      "value": "[-!]"
    },
    "_unary": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "unary_op"
            },
            {
              "type": "SYMBOL",
              "name": "_unary"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "prim"
        }
      ]
    },
    "prim": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "int_literal"
        }
      ]
    },
    "digit": {
      "type": "PATTERN",
      "value": "[0-9]+"
    },
    "hex_digit": {
      "type": "PATTERN",
      "value": "[0-9a-fA-F]+"
    },
    "binary_digit": {
      "type": "PATTERN",
      "value": "[01]+"
    },
    "octal_digit": {
      "type": "PATTERN",
      "value": "[0-7]+"
    },
    "int_literal": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "digit"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "0x"
            },
            {
              "type": "SYMBOL",
              "name": "hex_digit"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "0b"
            },
            {
              "type": "SYMBOL",
              "name": "binary_digit"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "0o"
            },
            {
              "type": "SYMBOL",
              "name": "octal_digit"
            }
          ]
        }
      ]
    }
  },
  "extras": [],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}