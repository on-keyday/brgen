{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "brgen",
  "rules": {
    "program": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_skip_lines"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_statement"
              },
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "_skip_white"
                  },
                  {
                    "type": "BLANK"
                  }
                ]
              }
            ]
          }
        }
      ]
    },
    "_skip_lines": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_space"
          },
          {
            "type": "SYMBOL",
            "name": "_comment"
          },
          {
            "type": "SYMBOL",
            "name": "_line"
          }
        ]
      }
    },
    "_skip_white": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_space"
          },
          {
            "type": "SYMBOL",
            "name": "_comment"
          },
          {
            "type": "SYMBOL",
            "name": "_line"
          }
        ]
      }
    },
    "_skip_space": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_space"
          },
          {
            "type": "SYMBOL",
            "name": "_comment"
          }
        ]
      }
    },
    "_statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "expr"
        }
      ]
    },
    "_space": {
      "type": "PATTERN",
      "value": "[ \\t]+"
    },
    "_line": {
      "type": "PATTERN",
      "value": "[\\r\\n]+"
    },
    "_comment": {
      "type": "PATTERN",
      "value": "#.*[\\r\\n]*"
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_]*"
    },
    "mul_op": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "*"
        },
        {
          "type": "STRING",
          "value": "/"
        },
        {
          "type": "STRING",
          "value": "%"
        },
        {
          "type": "STRING",
          "value": "&"
        },
        {
          "type": "STRING",
          "value": "<<<"
        },
        {
          "type": "STRING",
          "value": ">>>"
        },
        {
          "type": "STRING",
          "value": "<<"
        },
        {
          "type": "STRING",
          "value": ">>"
        }
      ]
    },
    "add_op": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "+"
        },
        {
          "type": "STRING",
          "value": "-"
        },
        {
          "type": "STRING",
          "value": "|"
        },
        {
          "type": "STRING",
          "value": "^"
        }
      ]
    },
    "cmp_op": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "=="
        },
        {
          "type": "STRING",
          "value": "!="
        },
        {
          "type": "STRING",
          "value": "<"
        },
        {
          "type": "STRING",
          "value": ">"
        },
        {
          "type": "STRING",
          "value": "<="
        },
        {
          "type": "STRING",
          "value": ">="
        }
      ]
    },
    "logical_and_op": {
      "type": "STRING",
      "value": "&&"
    },
    "logical_or_op": {
      "type": "STRING",
      "value": "||"
    },
    "expr": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PREC_LEFT",
          "value": 7,
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "expr"
              },
              {
                "type": "SYMBOL",
                "name": "_skip_space"
              },
              {
                "type": "SYMBOL",
                "name": "logical_or_op"
              },
              {
                "type": "SYMBOL",
                "name": "_skip_white"
              },
              {
                "type": "SYMBOL",
                "name": "expr"
              }
            ]
          }
        },
        {
          "type": "PREC_LEFT",
          "value": 8,
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "expr"
              },
              {
                "type": "SYMBOL",
                "name": "_skip_space"
              },
              {
                "type": "SYMBOL",
                "name": "logical_and_op"
              },
              {
                "type": "SYMBOL",
                "name": "_skip_white"
              },
              {
                "type": "SYMBOL",
                "name": "expr"
              }
            ]
          }
        },
        {
          "type": "PREC_LEFT",
          "value": 9,
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "expr"
              },
              {
                "type": "SYMBOL",
                "name": "_skip_space"
              },
              {
                "type": "SYMBOL",
                "name": "cmp_op"
              },
              {
                "type": "SYMBOL",
                "name": "_skip_white"
              },
              {
                "type": "SYMBOL",
                "name": "expr"
              }
            ]
          }
        },
        {
          "type": "PREC_LEFT",
          "value": 10,
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "expr"
              },
              {
                "type": "SYMBOL",
                "name": "_skip_space"
              },
              {
                "type": "SYMBOL",
                "name": "add_op"
              },
              {
                "type": "SYMBOL",
                "name": "_skip_white"
              },
              {
                "type": "SYMBOL",
                "name": "expr"
              }
            ]
          }
        },
        {
          "type": "PREC_LEFT",
          "value": 20,
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "expr"
              },
              {
                "type": "SYMBOL",
                "name": "_skip_space"
              },
              {
                "type": "SYMBOL",
                "name": "mul_op"
              },
              {
                "type": "SYMBOL",
                "name": "_skip_white"
              },
              {
                "type": "SYMBOL",
                "name": "expr"
              }
            ]
          }
        },
        {
          "type": "PREC_LEFT",
          "value": 30,
          "content": {
            "type": "SYMBOL",
            "name": "_unary"
          }
        }
      ]
    },
    "unary_op": {
      "type": "PATTERN",
      "value": "[-!]"
    },
    "_unary": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "unary_op"
            },
            {
              "type": "SYMBOL",
              "name": "_unary"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "postfix"
        }
      ]
    },
    "postfix": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "prim"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "member_access"
              },
              {
                "type": "SYMBOL",
                "name": "call"
              }
            ]
          }
        }
      ]
    },
    "member_access": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "."
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "call": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "args"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "args": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "expr"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "SYMBOL",
                "name": "expr"
              }
            ]
          }
        }
      ]
    },
    "prim": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "int_literal"
        }
      ]
    },
    "digit": {
      "type": "PATTERN",
      "value": "[0-9]+"
    },
    "hex_digit": {
      "type": "PATTERN",
      "value": "[0-9a-fA-F]+"
    },
    "binary_digit": {
      "type": "PATTERN",
      "value": "[01]+"
    },
    "octal_digit": {
      "type": "PATTERN",
      "value": "[0-7]+"
    },
    "int_literal": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "digit"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "0x"
            },
            {
              "type": "SYMBOL",
              "name": "hex_digit"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "0b"
            },
            {
              "type": "SYMBOL",
              "name": "binary_digit"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "0o"
            },
            {
              "type": "SYMBOL",
              "name": "octal_digit"
            }
          ]
        }
      ]
    }
  },
  "extras": [],
  "conflicts": [
    [
      "expr"
    ]
  ],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}