# spaning tree protocol

format BridgeID:
    priority :u16
    mac :u48

format BDPU:
    protocol_id :u16
    version :u8
    bpdu_type :u8
    flags :u8
    root_id :BridgeID
    root_path_cost :u32
    bridge_id :BridgeID
    port_id :u16
    message_age :u16
    max_age :u16
    hello_time :u16
    forward_delay :u16
